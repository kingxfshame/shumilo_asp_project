<div class="container">
    <table class="table">
        <thead class="thead-light">
            <tr>
                <th scope="col">#</th>
                <th scope="col">
                    Name in native language
                </th>
                <th scope="col">Name in a foreign language</th>
                <th scope="col">Sex</th>
            </tr>
        </thead>
        <tbody>
               @foreach (var x in ViewBag.names)
                {
                    <tr>
                        <th scope="row">@x.ID</th>
                        <td>@x.eestoni_nimi</td>
                        <td>@x.english_nimi</td>
                        <td>@x.sex</td>
                    </tr>

            }
        </tbody>
    </table>
</div>
